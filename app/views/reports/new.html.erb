<%= form_for(@report) do |f| %>
 
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title, autofocus: true %>
  </div>


  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :street_number %><br />
    <%= f.number_field :street_number %>
  </div>

   <div class="field">
    <%= f.label :street_name %><br />
    <%= f.text_field :street_name %>
  </div>

   <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>

   <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>

   <div class="field">
    <%= f.label :zip_code %><br />
    <%= f.number_field :zip_code %>
  </div>



  <div class="actions">
    <%= f.submit "Create Report" %>
  </div>
<% end %>